{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "olimpiadas-programacion": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/olimpiadas-programacion",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": [],
            "server": "src/main.server.ts",
            "outputMode": "server",
            "ssr": {
              "entry": "src/server.ts"
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all",
              "extraEnv": {
                "API_BASE_URL": "https://api.tusitio.com",
                "WS_BASE_URL": "wss://api.tusitio.com",
                "API_TIMEOUT": "10000",
                "RETRY_ATTEMPTS": "3",
                "RETRY_DELAY": "1000"
              }
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "extraEnv": {
                "API_BASE_URL": "http://127.0.0.1:8080",
                "WS_BASE_URL": "ws://127.0.0.1:8080",
                "API_TIMEOUT": "10000",
                "RETRY_ATTEMPTS": "3",
                "RETRY_DELAY": "1000"
              }
            },
            "local": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "extraEnv": {
                "API_BASE_URL": "http://127.0.0.1:8080",
                "WS_BASE_URL": "ws://127.0.0.1:8080",
                "API_TIMEOUT": "10000",
                "RETRY_ATTEMPTS": "3",
                "RETRY_DELAY": "1000"
              }
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "olimpiadas-programacion:build:production"
            },
            "development": {
              "buildTarget": "olimpiadas-programacion:build:development"
            },
            "local": {
              "buildTarget": "olimpiadas-programacion:build:local"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}